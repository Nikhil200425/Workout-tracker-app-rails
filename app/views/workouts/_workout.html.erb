<div id="<%= dom_id workout %>">
  <p>
    <strong>Title:</strong>
    <%= workout.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= workout.description %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= workout.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= workout.end_time %>
  </p>

  <p>
    <strong>User:</strong>
    <%= workout.user_id %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= workout.status %>
  </p>
</div>
